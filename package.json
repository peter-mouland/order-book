{
  "name": "mfe-worksjop",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": "22.x",
    "npm": "10.2.0"
  },
  "scripts": {
    "dev": "pnpm --filter order-book dev",
    "test": "pnpm -r test",
    "clean": "pnpm -r exec rm -rf node_modules dist build .turbo",
    "typecheck": "tsc --noEmit",
    "biome:all": "pnpm lint:fix && pnpm format:fix && check:fix",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write",
    "format": "biome format .",
    "format:fix": "biome format --write",
    "check": "biome check .",
    "check:fix": "biome check --write",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "workspaces": [
    "apps",
    "libs"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.18.12",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/browser-playwright": "^4.0.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "msw": "^2.11.6",
    "playwright": "^1.56.1",
    "stylelint": "^16.25.0",
    "stylelint-browser-compat": "1.0.0-beta.178",
    "stylelint-no-unused-selectors": "^1.0.40",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^4.0.4"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  }
}
